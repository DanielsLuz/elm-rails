<%= Elm::Compiler.compile(Dir.glob("app/assets/elm/**/*.elm")).html_safe %>

ElmWrapper = (function() {
  var elmEmbedObjects = [];

  return {
    addObject: function(objectId, object){
      elmEmbedObjects[objectId] = object;
    },

    getObject: function(objectId){
      console.log("returning object " + objectId);
      return elmEmbedObjects[objectId]
    }
  }
})();
